<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.khrystal.setd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".after.AfterFirstActivity" />
        <activity
            android:name=".after.AfterSecondActivity"
            android:label="@string/title_activity_after_second"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".pre.PreFirstActivity" />
        <activity
            android:name=".pre.PreSecondActivity"
            android:label="@string/title_activity_pre_second"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
